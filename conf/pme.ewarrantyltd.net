<VirtualHost *:80>
        ServerName pme.ewarrantyltd.net
        ServerAlias pme.ewarrantyltd.net
        DocumentRoot /var/www/ui_datatable_form
        <Directory />
                Options FollowSymLinks
                AllowOverride None
        </Directory>
        <Directory /var/www/>
                Options Indexes FollowSymLinks MultiViews
                AllowOverride None
                Order allow,deny
                allow from all
        </Directory>

        ScriptAlias /cgi-bin/ /usr/lib/cgi-bin/
        <Directory "/usr/lib/cgi-bin">
                AllowOverride None
                Options +ExecCGI -MultiViews +SymLinksIfOwnerMatch
                Order allow,deny
                Allow from all
        </Directory>

        ErrorLog ${APACHE_LOG_DIR}/error.log

        # Possible values include: debug, info, notice, warn, error, crit,
        # alert, emerg.
        LogLevel error

        CustomLog ${APACHE_LOG_DIR}/access.log combined
        AllowEncodedSlashes On

    Alias /doc/ "/usr/share/doc/"
    <Directory "/usr/share/doc/">
        Options Indexes MultiViews FollowSymLinks
        AllowOverride None
        Order deny,allow
        Deny from all
        Allow from 127.0.0.0/255.0.0.0 ::1/128
    </Directory>
   # WSGIDaemonProcess pme processes=2 threads=5 display-namepme maximum-requests=10000
   # WSGIProcessGroup ui_datatable_group
   # WSGIApplicationGroup %{GLOBAL}
    WSGIScriptAlias / /var/www/ui_datatable_form/pme.wsgi
   # WSGIPassAuthorization On

    Alias /media/  /var/www/ui_datatable_form/media/
    Alias /static/ /var/www/ui_datatable_form/static/
   <Directory /var/www/ui_datatable_form/static/>
    Order deny,allow
    Allow from all
    </Directory>

    <Directory /var/www/ui_datatable_form/media/>
    Order deny,allow
    Allow from all
    </Directory>

    <Directory /var/www/ui_datatable_form/>
#        AuthType Basic
#        AuthName "Site under testing"
#        Require valid-user
        Order deny,allow
        Allow from all
    </Directory>
</VirtualHost>


<VirtualHost 95.128.216.47:443>
        ServerName pme.ewarrantyltd.net

        DocumentRoot /home/pme/www/documents

        Alias /media/ /home/pme/www/documents/media/
        Alias /static/ /home/pme/www/documents/static/

        <Directory /home/pme/www/documents>
                Require all granted
        </Directory>

        <Directory /home/pme/www/documents/media>
                Require all granted
        </Directory>

        <Directory /home/pme/www/documents/static>
                Require all granted
        </Directory>


 #       WSGIDaemonProcess pme.ewarrantyltd.net processes=2 threads=15 display-name=%{GROUP}
 #       WSGIProcessGroup pme.ewarrantyltd.net

#        WSGIPythonPath /home/pme/.virtualenvs/pme_v1.0/lib/python2.7/site-packages
        WSGIScriptAlias / /home/pme/www/ui_datatable_folder/pme-v1.0.wsgi

        #<Directory /home/pme/www/wsgi-scripts>
        #       Order allow,deny
        #       Allow from all
        #</Directory>

        SSLEngine on

        SSLOptions +StrictRequire

        SSLCertificateFile /etc/ssl/certs/ewarrantyltd.net.crt
        SSLCertificateChainFile /etc/ssl/certs/sf_bundle-g2-g1.crt
        SSLCertificateKeyFile /etc/ssl/private/ewarranty-2048.key
</VirtualHost>
